{
  "name": "cache-docker-images-action",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "target": "node",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/dockerode": "^3.3.45"
  },
  "scripts": {
    "build": "bun run build:main && bun run build:post",
    "build:main": "bun build --minify --target=node --outdir=dist/ ./main.ts",
    "build:post": "bun build --minify --target=node --outdir=dist/ ./post.ts",
    "clean": "rm -rf ./dist",
    "test": "bun test --timeout 60000"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@actions/cache": "^4.1.0",
    "@actions/core": "^1.11.1",
    "dockerode": "^4.0.9"
  }
}
